<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="90.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AlchemyElementAdder.ClassPopUp">
   <children>
      <GridPane prefHeight="90.0" prefWidth="400.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="193.0" minWidth="10.0" prefWidth="87.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="319.0" minWidth="10.0" prefWidth="313.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="68.0" text="Name" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="7.0" />
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
               <children>
                     <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="58.0" text="id" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="7.0" />
               </children></AnchorPane>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
               <children>
                  <TextField fx:id="name" AnchorPane.leftAnchor="7.0" AnchorPane.topAnchor="3.0" prefHeight="25.0" prefWidth="200.0" />
               </children>
            </AnchorPane>
             <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="2">
                 <children>
                     <HBox alignment="CENTER_RIGHT" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="3.0">
                         <children>
                             <Button fx:id="cancelButton" alignment="CENTER_RIGHT" contentDisplay="CENTER" mnemonicParsing="false" text="Cancel" />
                             <Button fx:id="saveButton" mnemonicParsing="false" text="Save" />
                         </children>
                     </HBox>
                     <Label fx:id="errorLabel" prefHeight="17.0" prefWidth="126.0" textFill="RED" AnchorPane.leftAnchor="7.0" AnchorPane.topAnchor="7.0" />
                 </children>
             </AnchorPane>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <Label fx:id="idLabel" AnchorPane.leftAnchor="7.0" AnchorPane.topAnchor="7.0" />
               </children>
            </AnchorPane>
         </children>
      </GridPane>
   </children>
</AnchorPane>
